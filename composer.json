{
    "name": "bvoronov/zb-utils",
    "description": "Module providing common utils and assets",
    "type": "library",
    "version": "0.1.0",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.5",
        "zendframework/zendframework": "~2.4",
        "zendframework/zend-ldap": "^2.5",
        "zfcampus/zf-apigility": "~1.1",
        "zfcampus/zf-apigility-documentation": "^1.0.5",
        "zfcampus/zf-development-mode": "~2.0",
        "zfcampus/zf-apigility-doctrine": "^1.0",
        "doctrine/doctrine-orm-module": "^0.9.1",
        "zfr/zfr-cors": "1.*",
        "hashids/hashids": "^1.0"
    },
    "require-dev": {
        "phpunit/PHPUnit": "4.7.*",
        "zendframework/zftool": "dev-master",
        "zendframework/zend-developer-tools": "dev-master",
        "zfcampus/zf-apigility-admin": "^1.3.1",
        "zfcampus/zf-deploy": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "ZB\\Utils\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
      "psr-4": {
          "ZBTest\\Utils\\": "test/"
      }
    }
}
